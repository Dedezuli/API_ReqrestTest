{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "jsx": "react",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "allowJs": true,
    "types": ["cypress"],
    "baseUrl": "./cypress",
    "paths": {
      "utils/*": ["./utils/*"],
      "fixtures/*": ["./fixtures/*"],
      "support/*": ["./support/*"]
    }
  },
  "include": ["**/*.*"],
  "sourceMap": true,
  "exclude": ["node_modules", "**/node_modules/*"]
}
